{
    "name": "farisc0de/phpmigration",
    "description": "Migration Library for PHP",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Farisc0de\\PhpMigration\\": "src/"
        }
    },
    "authors": [
        {
            "name": "Faris AL-Otaibi",
